[Desktop Entry]
Name=Emacs (Client)
GenericName=Text Editor
Comment=Edit text
Exec=emacsclient -c --eval "(call-interactively #'+workspace/delete)" --alternate-editor= %F
Icon=emacs
Type=Application
Terminal=false
Categories=Development;TextEditor;
StartupNotify=true
StartupWMClass=Emacs
Keywords=emacsclient;
Actions=new-window;new-instance;

[Desktop Action new-window]
Name=New Window
Exec=emacsclient -c --alternate-editor= %F

[Desktop Action new-instance]
Name=New Instance
Exec=emacs %F
